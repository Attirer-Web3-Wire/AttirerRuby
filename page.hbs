{{!< default}} <div class="page-wrapper" style="display: flex;flex-direction: column;align-items: center;justify-content: center;">
    {{#post}}
    <article class="service-page">
    {{#match @page.show_title_and_feature_image}}
    <h1 class="title-cl">
    {{title}}
    </h1>
    {{#if feature_image}}
      <figure class='kg-image-card'>
        <img
          alt="{{#if feature_image_alt}}{{ feature_image_alt }}{{/if}}"
          class='c-feature-image lazyload'
          data-src='{{ img_url feature_image size='feature-image-large' }}'
          style = 'width:100%;'
        >
      </figure>
      {{#if feature_image_caption}}
        <figcaption class='c-feature-image-caption'>{{ feature_image_caption }}</figcaption>
      {{/if}}
 
    {{/if}}
    {{/match}}
        
        {{content}}
    </article>
    {{/post}}

    </div>