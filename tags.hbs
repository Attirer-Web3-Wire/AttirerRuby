{{!< default}} <header class='c-page-header-tags'>
    <div class='o-grid o-grid--2-columns'>
        <div>
            <h1 class='c-topper__headline u-font-headline u-font-family-sansSerif'>All Tags</h1>
        </div>
    </div>

    <div class='js-grid o-grid o-grid--3-columns'>
        {{#get 'tags' limit='all'}}
        {{#foreach tags}}
        {{> card-tag }}
        {{/foreach}}
        {{/get}}
    </div>
    </header>