<header class='new-header'>
    <div class="top-most">
<div class="light-widget">
<script async src="https://public.bnbstatic.com/unpkg/growth-widget/cryptoCurrencyWidget@0.0.9.min.js" ></script><div class="binance-widget-marquee" data-cmc-ids="1,1027,1839,52,74,5426,3890,5805,7083,5994,2010,24478" data-theme="light" data-transparent="true" data-locale="en" data-powered-by="Powered by" data-disclaimer="Disclaimer" ></div>
</div>
<div class="dark-widget">
<script async src="https://public.bnbstatic.com/unpkg/growth-widget/cryptoCurrencyWidget@0.0.9.min.js" ></script><div class="binance-widget-marquee" data-cmc-ids="1,1027,1839,52,74,5426,3890,5805,7083,5994,2010,24478" data-theme="dark" data-transparent="true" data-locale="en" data-powered-by="Powered by" data-disclaimer="Disclaimer" ></div>
</div>
</div>

<div class="second-top">
    <div class='c-header__top-block c-header__top-center'>
        {{> logo }}
    </div>
</div>
    <div class='c-header__top'>

        <div class='o-grid c-header__top-inner'>
            
            <div class='c-header__top-block c-header__top-left' style="justify-content:flex-start;">

            {{#if @custom.enable_dark_mode}}
                <button
                    class='c-header__button c-header__button--colorMode c-color-mode-btn js-mode-toggle js-mode-toggle-title'
                    data-colormode='light'>
                    <span class='js-mode-toggle-text u-visible-on-desktop'>{{t 'Dark' }}</span>
                </button>
            {{/if}}
                <button class='c-header__button c-header__button--search' data-ghost-search>{{> icons/search }}<span
                        class='u-visible-on-desktop'>{{t 'Search' }}</span></button>
            </div>

            <div class='c-header__top-block c-header__top-center'>
{{> logo }}
            </div>

            <div class='c-header__top-block c-header__top-right'>
                {{#if @site.members_enabled}}
                {{#if @member}}
                <a href='#/portal/account' class='c-header__button'>{{t 'Account' }}</a>
                {{else}}
                <a href='#/portal/signin' class='c-header__button u-visible-on-desktop'>{{t 'Sign In' }}</a>
                {{#unless @site.members_invite_only}}
                <a href='#/portal/signup' class='c-btn c-btn--small'>{{t 'Subscribe' }}</a>
                {{/unless}}
                {{/if}}
                {{/if}}
            </div>

        </div>
    </div>


    <div class='bottom-header'>
        {{navigation}}
    </div>

</header>